{
  "name": "eel-wasm",
  "version": "0.0.15",
  "type": "module",
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "source": "src/index.ts",
  "targets": {
    "main": {
      "outputFormat": "esmodule",
      "isLibrary": true
    }
  },
  "license": "MIT",
  "homepage": "https://github.com/captbaritone/eel-wasm",
  "author": "Jordan Eldredge <jordan@jordaneldredge.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/captbaritone/eel-wasm.git"
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest",
    "ci": "pnpm test && pnpm build",
    "benchmark": "node tools/benchmark/performance.js perf",
    "consistency": "node tools/benchmark/performance.js consistency",
    "test-parse": "node tools/parseMilk.js --stats --summary --dir=./fixtures/mega",
    "test-compile": "node tools/parseMilk.js --stats --summary --compile --dir=./fixtures/mega",
    "build-parser": "node ./tools/buildParser.js > ./build/parser.js",
    "build": "pnpm build-parser && parcel build",
    "clean": "rm -rf build/*",
    "prepublishOnly": "pnpm test && pnpm clean && pnpm build"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "jest": "^29.7.0",
    "jison": "^0.4.18",
    "milkdrop-eel-parser": "^0.0.4",
    "milkdrop-preset-utils": "^0.1.0",
    "parcel": "^2.12.0",
    "prettier": "^1.19.1",
    "print-diff": "^1.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "wabt": "^1.0.12",
    "yargs": "^15.1.0"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "eel"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "dist"
    ],
    "transform": {
      "^.+\\.(ts|js)$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "transformIgnorePatterns": [
      "node_modules/(?!.*\\.mjs$)"
    ]
  }
}
